%h2= current_repo.name

.attributes
  External Id:
  = current_repo.external_id

.users
  Users:
  = current_repo.users.collect {|u| u.name}.join(',')

.commits
  - current_repo.git_commits.limit(15).each do |gc|
    .image
      - if gc.user
        .name
          %b= link_to gc.user.name, "http://github.com/#{gc.user.name}"
      - if current_user && gc.image && gc.repo.users.include?(current_user)
        = image_tag gc.image
      - else
        = image_tag gc.raw
